{
  "manifest_version": 3,
  "name": "LinkedIn Crystal Clarity",
  "version": "1.0",
  "description": "Transforms LinkedIn's AI enthusiasm into something more... crystalline",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.linkedin.com/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
